        <name>Get Table Schema</name>
        <operation_script><![CDATA[(function process(/*RESTAPIRequest*/ request, /*RESTAPIResponse*/ response) {

	var pkg = new x_108443_sndml.TableSchema();
	gs.info('GetTableSchema ' + request.pathParams.tablename);
	var schema = pkg.getSchema(request.pathParams.tablename);
	// schema should be an array of field definitions
	if (schema && schema.length > 0) {
		response.setBody(schema);		
	}
	else {
		response.setStatus(404);
	}

})(request, response);]]></operation_script>
